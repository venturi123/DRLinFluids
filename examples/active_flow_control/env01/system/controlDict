FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pimpleFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.005;

deltaT          0.0005;

writeControl    runTime;

writeInterval   0.005;

purgeWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  no;

maxCo           1;

//maxDeltaT       0.1;

libs ("libinletParabolicVelocity.so" "libjetParabolicVelocity.so" "libincrementalJetParabolicVelocity.so");
// ************************************************************************* //

functions
{
    #includeFunc  CourantNo
    #includeFunc  residuals
    #includeFunc  vorticity
    #includeFunc  forceCoeffsIncompressible
    #includeFunc  forcesIncompressible
    #includeFunc  yPlus
    #includeFunc  probes
}


